{
  "name": "api",
  "version": "1.13.0",
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "yarn dotenv && yarn types && yarn tsoa && yarn dynamodb && yarn openapi && yarn build",
    "build": "tsc",
    "start": "serverless offline start",
    "deploy": "serverless deploy",
    "serverless": "SLS_DEBUG=* serverless",
    "dotenv": "dotenv -e .scaffoldly/.env -c $NODE_ENV -- ngx-scripts env service_urls env_vars --parse-json --out src/env.ts",
    "openapi": "openapi-generator -g axios -i .scaffoldly/$NODE_ENV -o src/services/openapi -r auth",
    "types": "ts-node types.ts",
    "tsoa": "node tsoa.js",
    "dynamodb": "serverless dynamodb install",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "devDependencies": {
    "@ngx-rocket/scripts": "^5.1.0",
    "@scaffoldly/openapi-generator": "^1.0.12",
    "@types/aws-lambda": "^8.10.72",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.3.0",
    "serverless": "^2.2.0",
    "serverless-bundle": "^4.2.3",
    "serverless-configuration": "^1.0.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.8.0",
    "serverless-offline-dotenv": "^0.1.2",
    "serverless-plugin-resource-tagging": "^1.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@scaffoldly/serverless-util": "^3.0.33",
    "@vendia/serverless-express": "^4.3.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "joi-to-typescript": "^1.12.0",
    "tsoa": "^3.8.0",
    "ulid": "^2.3.0"
  }
}
